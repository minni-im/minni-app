{
  "name": "minni-app",
  "version": "0.1.0",
  "description": "Anywhere should be the place to be working from",
  "main": "index.js",
  "scripts": {
    "test": "jest --silent",
    "svg": "svg2react",
    "watch:backend": "babel app-backend/ -d dist/lib --watch --copy-files --ignore __mocks__,test.js",
    "watch:frontend": "webpack --progress --watch --debug",
    "build:backend": "rimraf dist/lib && babel app-backend/ -d dist/lib --ignore __mocks__,test.js && copyfiles --up 1 app-backend/views/*.pug dist/lib",
    "build:frontend": "rimraf dist/public/js && webpack --progress",
    "css:style": "lessc assets/stylesheets/style.less dist/public/css/style.css --autoprefix='last 2 versions' --clean-css='advanced'",
    "css:chat": "lessc assets/stylesheets/chat.less dist/public/css/chat.css --autoprefix='last 2 versions' --clean-css='advanced'",
    "css:mobile": "lessc assets/stylesheets/mobile.less dist/public/css/mobile.css --autoprefix='last 2 versions' --clean-css='advanced'",
    "build:css": "npm run css:style && npm run css:chat && npm run css:mobile",
    "static": "copyfiles -u 1 -e './public/images/svgs/*.react.js' './public/**/*' ./dist/public && copyfiles -u 2 './assets/favicons/*' ./dist/public",
    "dist": "npm run build:backend && NODE_ENV=production npm run build:frontend && NODE_ENV=production npm run build:css && npm run static"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minni-im/minni-app.git"
  },
  "keywords": [
    "chat",
    "group",
    "chat",
    "im",
    "instant",
    "messaging",
    "real-time"
  ],
  "author": "Benoit Charbonnier <benoit.charbonnier@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minni-im/minni-app/issues"
  },
  "homepage": "https://github.com/minni-im/minni-app#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-preset-node8": "1.2.0",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^2.3.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "jest": "21.2.1",
    "json-loader": "0.5.7",
    "less": "^3.12.2",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "react-svg-loader": "1.1.1",
    "svg2react": "^0.5.2",
    "webpack": "3.6.0",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-sources": "^1.4.3"
  },
  "dependencies": {
    "@minni-im/emojify": "^1.0.0",
    "@minni-im/minni-auth-github": "^1.0.0",
    "@minni-im/minni-embed": "1.1.1",
    "@minni-im/minni-embed-youtube": "1.0.2",
    "babel-cli": "6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-register": "^6.22.0",
    "bcryptjs": "^2.4.0",
    "body-parser": "1.18.2",
    "classnames": "^2.1.5",
    "click-outside": "2.0.2",
    "combokeys": "3.0.0",
    "connect-redis": "3.3.2",
    "cookie-parser": "^1.3.5",
    "deep-extend": "^0.5.0",
    "express-session": "1.15.6",
    "express.oi": "0.0.21",
    "flash": "^1.1.0",
    "flux": "3.1.3",
    "glob": "^7.1.1",
    "highlight.js": "^9.9.0",
    "immutable": "^3.7.6",
    "js-yaml": "3.10.0",
    "keymirror": "^0.1.1",
    "lodash.isplainobject": "^4.0.6",
    "long": "^3.0.1",
    "moment": "^2.17.1",
    "node-fetch": "1.7.3",
    "node-libs-browser": "^2.0.0",
    "node_hash": "^0.2.0",
    "object-path": "^0.11.3",
    "passport": "0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "prop-types": "15.6.0",
    "pug": "2.0.0-rc.4",
    "react": "16.0.0",
    "react-document-title": "^2.0.2",
    "react-dom": "16.0.0",
    "react-intersection-observer": "1.1.1",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-transition-group": "2.2.1",
    "redis": "2.8.0",
    "request": "2.83.0",
    "simple-markdown": "0.3.1",
    "tape-recorder": "^2.1.1",
    "twitter-text": "1.14.7"
  }
}
